{
  "name": "v360-website",
  "version": "1.0.0",
  "description": "V.360 - Vanderbilt Athletics Development Platform",
  "scripts": {
    "start": "live-server",
    "build": "npm run minify",
    "minify": "npm run minify:css && npm run minify:js",
    "minify:css": "cleancss -o dist/css/styles.min.css css/styles.css",
    "minify:js": "terser js/*.js -o dist/js/main.min.js"
  },
  "dependencies": {},
  "devDependencies": {
    "clean-css-cli": "^5.6.2",
    "live-server": "^1.2.2",
    "terser": "^5.19.2"
  }
}